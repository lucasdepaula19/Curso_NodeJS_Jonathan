<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        <% films.forEach(f => { %>
            <tr>
                <td><%= f.film_id %>></td>
                <td><%= f.title %></td>
            </tr>

        <% }) %>
    </tbody>
</table>